    "@material-ui/styles": "^5.0.0-alpha.25",
    "@relyzer/shared": "^1.0.0-alpha.1",
    "comment-parser": "^1.1.1",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/traverse": "^7.12.12",
    "@types/babel__core": "^7.1.12",
    "@types/lodash": "^4.14.168",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "babel-plugin-tester": "^10.0.0",
    "lerna": "^3.22.1",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {
    "@relyzer/runtime": "^1.0.0"
  }
